meta {
  name: Get Photo by ID Weight
  type: http
  seq: 6
}

get {
  url: {{baseUrl}}/weights/{{id_weight}}/photo
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

script:post-response {
  if (res.status === 200) {
    const responseData = res.getBody();
    
    bru.setEnvVar("id_weight", responseData.id);
    
    console.log("Id Weight saved:", responseData.id);
  }
}
