meta {
  name: 02 - Get Goal by ID
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/goals/{{lastGoalId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

tests {
  test("Goal retrieved successfully", function() {
    expect(res.status).to.equal(200);
    expect(res.body.id).to.be.a('number');
    expect(res.body.targetWeight).to.exist;
    expect(res.body.targetDate).to.exist;
    expect(res.body.userId).to.be.a('number');
    expect(res.body.createdAt).to.exist;
  });
}