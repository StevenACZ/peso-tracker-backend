meta {
  name: Get Photo by ID
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/photos/{{id_photo}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

script:post-response {
  if (res.status === 200) {
    const responseData = res.getBody();
    
    bru.setEnvVar("id_photo", responseData.id);
    
    console.log("Id Photo saved:", responseData.id);
  }
}
