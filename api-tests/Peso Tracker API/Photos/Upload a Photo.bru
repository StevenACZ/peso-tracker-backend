meta {
  name: Upload a Photo
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/photos/upload
  body: multipartForm
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:multipart-form {
  weightId: {{id_weight}}
  notes: 
  photo: @file(/Users/steven/Downloads/Screenshot 2025-07-24 at 18.03.57.jpg)
}

script:post-response {
  if (res.status === 201) {
    const responseData = res.getBody();
    
    bru.setEnvVar("id_photo", responseData.id);
    
    console.log("Id Photo saved:", responseData.id);
  }
}
