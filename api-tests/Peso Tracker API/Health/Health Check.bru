meta {
  name: Health Check
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/health
  body: none
  auth: none
}

tests {
  test("Health check successful", function() {
    expect(res.status).to.equal(200);
    expect(res.body.status).to.equal("ok");
    expect(res.body.timestamp).to.exist;
    expect(res.body.uptime).to.be.a('number');
  });
}