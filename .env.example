# Development Environment Configuration
NODE_ENV=development
PORT=3000

# CORS Configuration - Development
FRONTEND_URL=http://localhost:3000

# Rate Limiting - Development (more permissive)
RATE_LIMIT_TTL=300
RATE_LIMIT_LIMIT=1000

# Bcrypt Configuration - Development (faster)
BCRYPT_SALT_ROUNDS=10

# JWT Configuration
JWT_EXPIRES_IN=24h

# Storage Configuration
SUPABASE_STORAGE_BUCKET=peso-tracker-photos

# Add your production database and Supabase configuration here
DATABASE_URL="postgresql://postgres.[YOUR-PROJECT-REF]:[YOUR-PASSWORD]@aws-0-[REGION].pooler.supabase.com:5432/postgres?pgbouncer=true"
DIRECT_URL="postgresql://postgres.[YOUR-PROJECT-REF]:[YOUR-PASSWORD]@aws-0-[REGION].pooler.supabase.com:5432/postgres"
SUPABASE_URL="https://[YOUR-PROJECT-REF].supabase.co"
SUPABASE_ANON_KEY="[YOUR-ANON-KEY]"
SUPABASE_SERVICE_ROLE_KEY="[YOUR-SERVICE-ROLE-KEY]"
JWT_SECRET="your-super-secret-jwt-key-min-32-characters"